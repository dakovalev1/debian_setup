{
    "latex-workshop.latex.autoBuild.run": "onSave",
    "files.autoSave": "off",
    "scm.alwaysShowRepositories": true,
    "scm.alwaysShowActions": true,
    "git.untrackedChanges": "separate",
    "editor.suggest.snippetsPreventQuickSuggestions": true,
    "latex-workshop.latex.clean.method": "glob",
    "latex-workshop.latex.clean.fileTypes": [
        "%DOCFILE%.aux",
        "%DOCFILE%.bbl",
        "%DOCFILE%.blg",
        "%DOCFILE%.idx",
        "%DOCFILE%.ind",
        "%DOCFILE%.lof",
        "%DOCFILE%.lot",
        "%DOCFILE%.out",
        "%DOCFILE%.toc",
        "%DOCFILE%.acn",
        "%DOCFILE%.acr",
        "%DOCFILE%.alg",
        "%DOCFILE%.glg",
        "%DOCFILE%.glo",
        "%DOCFILE%.gls",
        "%DOCFILE%.fls",
        "%DOCFILE%.log",
        "%DOCFILE%.fdb_latexmk",
        "%DOCFILE%.snm",
        "%DOCFILE%.synctex(busy)",
        "%DOCFILE%.synctex.gz(busy)",
        "%DOCFILE%.nav",
        "%DOCFILE%.vrb",
        "%DOCFILE%.synctex.gz"
    ],
    "editor.wordWrap": "on",
    "latex-workshop.intellisense.package.dirs": [
        "/home/user/repository/latex/dev/packages"
    ],
    "keyboard.dispatch": "keyCode",
    "python.envFile": "/root/getifaddrs_skip/.env"
}
